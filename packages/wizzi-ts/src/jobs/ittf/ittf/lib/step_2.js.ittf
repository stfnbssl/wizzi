module
	kind jsfile

	var horses
		[
			$foreach item in Object.values($.getCtx('horse'))
				@ '${item}'
	
	class Animal
		m sayHello
			info this.cry, ', I am ', this.name

	class Horse
		super Animal
		ctor
			param name
			set this.name = name
			set this.cry = 'Hiii'

	var horse = new Horse('Varenne')
	_ horse.sayHello