module
	kind jsfile

	$
		$.setCtx('horse.ribot', 'Ribot');

	class Animal
		m sayHello
			info this.cry, ', I am ', this.name

	class Horse
		super Animal
		ctor
			param name
			set this.name = name
			set this.cry = 'Hiii'

	var horse = new Horse('Varenne')
	_ horse.sayHello

	$
		$.setCtx('horse.varenne', 'Varenne');
